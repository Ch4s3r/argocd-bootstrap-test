{
    "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.15.0/.schema/devbox.schema.json",
    "packages": [
      "kubectl",
      "kubernetes-helm"
    ],
    "shell": {
      "scripts": {
        "bootstrap": [
            "kubectl kustomize --enable-helm apps/argocd | kubectl apply -f -"
        ]
      }
    }
  }
  